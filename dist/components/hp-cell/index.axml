<view className={{className}}>
    <view className="hp-cell-header">
        <view className="hp-cell-header-left">
            <text className="label">{{label}}</text>
        </view>
        <view a:if={{arrow}} className="hp-cell-header-right">
            >
        </view>
    </view>
    <view a:if={{description}} className="hp-cell-content">{{description}}</view>
</view>